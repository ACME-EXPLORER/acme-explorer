{
  "name": "acme-explorer-frontend",
  "version": "1.0.0",
  "description": "Acme explorer frontend to mange custom trips for explorers",
  "main": "src/index.js",
  "author": "acme-explorer",
  "license": "MIT",
  "scripts": {
    "serve": "webpack serve --config config/webpack.config.js --mode=development",
    "dev": "webpack --config config/webpack.config.js --mode=development --watch",
    "build": "webpack --config config/webpack.config.js --mode=production --node-env=production",
    "build:dev": "webpack --config config/webpack.config.js --mode=development",
    "lint": "semistandard src/",
    "lint:fix": "semistandard --fix src/",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --testMatch='**/src/**/*.spec.js' --coverage",
    "test:e2e": "cross-env npx cypress run"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "firebase": "^8.9.1",
    "font-awesome": "^4.7.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap-sweetalert": "^5.2.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.1.1",
    "react-toastify": "^8.1.0",
    "reactstrap": "^9.0.1",
    "redux": "^4.1.2",
    "styled-components": "^5.3.3",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@faker-js/faker": "^5.5.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@reduxjs/toolkit": "^1.7.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cypress": "^9.7.0",
    "jest": "^27.4.3",
    "postcss-loader": "^6.2.1",
    "pre-commit": "^1.2.2",
    "react-refresh": "^0.11.0",
    "semistandard": "^16.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "semistandard": {
    "env": [
      "jest"
    ],
    "global": [
      "localStorage",
      "location",
      "FormData"
    ],
    "ignore": [
      "src/modules/login/ConfirmationPage.js"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/syntax-dynamic-import",
      "@babel/transform-runtime"
    ],
    "presets": [
      [
        "@babel/preset-env"
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "postcss": {
    "plugins": [
      [
        "autoprefixer"
      ]
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
