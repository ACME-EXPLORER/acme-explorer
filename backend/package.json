{
  "name": "acme-explorer-backend",
  "version": "1.0.1",
  "description": "Acme explorer backend to mange custom trips for explorers",
  "main": "src/app.js",
  "type": "module",
  "author": "acme-explorer",
  "license": "MIT",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon",
    "format": "prettier --write '{src,test}/**/*.js'",
