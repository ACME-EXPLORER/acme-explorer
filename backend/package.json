  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "async": "^3.2.3",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dateformat": "^5.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validator": "^6.14.0",
    "firebase-admin": "^10.0.2",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.0.15",
    "mongoose-to-swagger": "^1.4.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.1",
    "redis": "^4.0.4",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "babel-eslint": "10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "supertest": "^6.1.6"
  },
  "jest": {
    "transform": {}
  }
